// for loop

main:function integer () = {
	
    i: integer = 0;

    for (i = 0; i < 4; i++) {
        print i;
    }
    
    print "\n";
    i = 0;
    for (; i < 4; i++) {
        print i;
    }
    print "\n";

    for (i=0; i < 4; ) {
        print i;
        i = i+1;
    }
    print "\n";
    
}
